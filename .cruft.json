{
  "template": "https://github.com/lkubb/salt-template-formula-compose",
  "commit": "c5351b98a6ab88feb768fe27bbcad6b148c56910",
  "checkout": null,
  "context": {
    "cookiecutter": {
      "_extensions": [
        "local_extensions.YAMLDumper"
      ],
      "_jinja2_env_vars": {
        "block_start_string": "{!",
        "block_end_string": "!}",
        "variable_start_string": "{=",
        "variable_end_string": "=}",
        "comment_start_string": "{@",
        "comment_end_string": "@}"
      },
      "name": "Eclipse Mosquitto",
      "abbr": "mosquitto",
      "abbr_pysafe": "mosquitto",
      "project_name": "mosquitto",
      "containers": "mosquitto",
      "git_username": "lkubb",
      "_lookup": {
        "containers": {
          "mosquitto": {
            "image": "docker.io/library/eclipse-mosquitto"
          }
        },
        "paths": {
          "config": "config",
          "data": "data",
          "log": "log"
        }
      },
      "_settings": {
        "config": {
          "protocol": "mqtt",
          "log_dest": "stdout",
          "log_type": [
            "error",
            "warning",
            "notice",
            "information"
          ],
          "persistence": true,
          "listener 1883": {
            "protocol": "mqtt"
          }
        }
      },
      "_template": "https://github.com/lkubb/salt-template-formula-compose"
    }
  },
  "directory": null
